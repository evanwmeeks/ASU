public class Box {
	
    private double height, width, length; 
    
    public Box(double height, double width, double length) {
        this.height = height; 
        this.width = width; 
        this.length = length;
    }
    
    
    public static void isTall(Box x){
        if ((x.height > x.width) && (x.height > x.length)) {
            System.out.print("Hello");
        }

    }
    
    
}
